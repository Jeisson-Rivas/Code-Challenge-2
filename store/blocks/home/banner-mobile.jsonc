        },
      
        "image#banner-casa-ideas-mobile": {
          "props": {
            "src": "assets/images/Casaideas.svg",
            "maxHeight": 500
          }
        },
      
          "flex-layout.col#banner-ondademar-mobile": {
          "title": "Banner secundario",
          "children": ["image#banner-ondademar-mobile"]
        },
      
        "image#banner-ondademar-mobile": {
          "props": {
            "src": "assets/images/ondademar.svg",
            "maxHeight": 500
          }
        }
}