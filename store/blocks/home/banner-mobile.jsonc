        "children": ["flex-layout.row#banner-dos-banners-mobile" ]
      },
      
      
        "flex-layout.row#banner-dos-banners-mobile": {
          "title": "Banner secundario y vitrina",
          "props":{
            "preserveLayoutOnMobile": true
          },
          "children": [
            "flex-layout.col#banner-casa-ideas-mobile",
            "flex-layout.col#banner-ondademar-mobile"
          ]
        },
      
        "flex-layout.col#banner-casa-ideas-mobile": {
